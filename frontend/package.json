{
  "name": "lunareclipse-frontend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:clean": "npm run clean && npm run build",
    "build:analyze": "cross-env ANALYZE=true npm run dev",
    "build:storybook": "storybook build -o build/storybook",
    "clean": "rimraf ./build",
    "dev": "next dev",
    "dev:clean": "rimraf package-lock.json node_modules && npm install && npm run dev",
    "format": "prettier --write .",
    "format:check": "prettier --cache --check .",
    "i18n:update": "i18next-resources-for-ts interface -i ./src/i18n/en -o ./src/types/i18next.resources.d.ts",
    "lint": "next lint",
    "size:check": "npm run build && size-limit --why",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "tsc:check": "tsc",
    "test": "jest --watch",
    "test:all": "npm-run-all --parallel tsc:check test lint format:check",
    "test:coverage": "jest --collectCoverage"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "accept-language": "^3.0.18",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "i18next": "^23.9.0",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-resources-to-backend": "^1.2.0",
    "next": "14.1.0",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-cookie": "7.0.2",
    "react-dom": "^18",
    "react-i18next": "^14.0.5",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tslog": "^4.8.4-0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@jest/globals": "^29.7.0",
    "@next/bundle-analyzer": "^14.1.0",
    "@nkzw/eslint-config": "^1.10.0",
    "@size-limit/file": "^11.0.1",
    "@size-limit/webpack": "^11.0.1",
    "@size-limit/webpack-why": "^11.0.1",
    "@storybook/addon-essentials": "^8.0.2",
    "@storybook/addon-interactions": "^8.0.2",
    "@storybook/addon-links": "^8.0.2",
    "@storybook/blocks": "^8.0.2",
    "@storybook/nextjs": "^8.0.2",
    "@storybook/react": "^8.0.2",
    "@storybook/test": "^8.0.2",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "i18next-resources-for-ts": "1.5.0",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "msw": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "rimraf": "^5.0.5",
    "size-limit": "^11.0.1",
    "storybook": "^8.0.2",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "Chrome 118",
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": "build/server/*.js",
      "limit": "60 kB"
    },
    {
      "path": "build/server/app/*.js",
      "limit": "10 kB"
    }
  ],
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
